{
    "name": "latrell/swagger",
    "description": "Swagger for Laravel 5",
    "keywords": ["L5", "Laravel 5", "Laravel", "Swagger", "Swagger-PHP", "Swagger-UI"],
    "license": "MIT",
    "authors": [
        {
            "name": "Latrell Chan",
            "email": "i@latrell.me"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*",
        "illuminate/view": "5.*",
        "illuminate/config": "5.*",
        "illuminate/console": "5.*",
        "zircote/swagger-php": "1.*|2.*"
    },
    "require-dev": {
		"phpunit/phpunit": "4.*",
		"mockery/mockery": "*"
	},
    "autoload": {
    		"classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "Latrell\\Swagger\\": "src/"
        }
    },
    "autoload-dev": {
		"files": [
			"tests/TestCase.php"
		]
	},
    "minimum-stability": "stable"
}
